# 表单组件
'.text.html.mustache':
	'WeUI按钮区域':
		'prefix': 'weui-btn-area'
		'body': """
			<view class="weui-btn-area">
				<button class="weui-btn" type="${1:primary}" ${2:hover-class="none" }bindtap="${3:btntap}">确定</button>
			</view>
		"""
	'WeUI按钮':
		'prefix': 'weui-btn'
		'body': '<button class="weui-btn" type="${1:primary}" ${2:hover-class="none" }bindtap="${3:btntap}">$4</button>'
	
